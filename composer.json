{
    "name": "jayen/number-converter",
    "description": "Laravel package to convert numbers written in words into integers.",
    "type": "library",
    "homepage": "https://github.com/jayenbambharoliya/number-converter",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Jayen\\NumberConverter\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Jayen Bambhroliya",
            "email": "jayenbambharoliya@gmail.com",
            "role": "Developer"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Jayen\\NumberConverter\\NumberConverterServiceProvider"
            ],
            "aliases": {
                "NumberConverter": "Jayen\\NumberConverter\\Facades\\NumberConverter"
            }
        }
    },
    "require": {
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "php": ">=8.1"
    },
    "require-dev": {
        "pestphp/pest": "^3.8"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
