    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Jayen\\NumberConverter\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Jayen Patel",
            "email": "jayenbambharoliya@gmail.com"
        }
    ],
